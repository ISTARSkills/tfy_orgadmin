<div id="publishLesson" class="modal fade" tabindex="-1" role="dialog">
	<div class="modal-dialog" role="document">
		<div class="modal-content custom-modal-content">
			<div class="modal-header custom-modal-header">
				<h5 class="modal-title custom-modal-title" id="loModalLabel">Publishing
					Status</h5>
				<button type="button" class="close" data-dismiss="modal"
					aria-label="Close">
					<span aria-hidden="true">×</span>
				</button>
			</div>
			<div class="modal-body custom-no-padding">
				<div class="container-fluid">
					<div class='row'>
						<ul class="list-group" style="margin: auto;">
							<li class="list-group-item list-group-item-primary"
								style="margin-top: 10px; text-align: center;"><b>Publishing
									Checks</b></li>
							<li class="list-group-item" id='isTitle'>Lesson
								has title</li>
							<li class="list-group-item" id='isDesc'>Lesson
								has Description</li>
							<li class="list-group-item" id='isImage'>Lesson
								has Image</li>
							<li class="list-group-item" id='isAssets'>Lesson
								has some content</li>
							<li class="list-group-item" id='isLos'>Lesson
								has learning objectives</li>
						</ul>
					</div>
					<!-- <div class='row'>
						<button class="btn btn-sm btn-modal" style="margin: 20px auto;"
							id='changeSessionModalSubmit' id='publishLessonButton' disabled>Publish
							Lesson</button>
					</div> -->
				</div>
			</div>
		</div>
	</div>
</div>